{
  "name": "my-first-friendly-guacamole-site",
  "author": "Guacamole Hero",
  "license": "ISC",
  "version": "0.0.1",
  "main": "gulpfile.js/index.js",
  "scripts": {
    "watch": "npm run build:pre && gulp watch",
    "cc": "npm run create:component $args",
    "create:component": "php src/_core/scripts/create.component.php $args",
    "cp": "npm run create:page $args",
    "create:page": "php src/_core/scripts/create.page.php $args",
    "cl": "npm run create:layout $args",
    "create:layout": "php src/_core/scripts/create.layout.php $args",
    "build": "gulp build",
    "build:pre": "npm run build:vendor",
    "build:vendor": "php src/_core/scripts/build.vendor.scss.php && php src/_core/scripts/build.vendor.js.php"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^4.0.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-pupa": "^1.0.0",
    "gulp-replace": "^1.1.2",
    "gulp-uglify": "^3.0.2",
    "gulp-watch": "^5.0.1"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "jquery": "^3.6.0",
    "run": "^1.4.0",
    "watch": "^1.0.2"
  }
}
